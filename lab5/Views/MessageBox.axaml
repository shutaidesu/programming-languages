<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
		d:DesignWidth="250" d:DesignHeight="200"
		Width="250" Height="200"
		CanResize="false" 
        x:Class="lab5.MessageBox"
        Title="MessageBox">
	<StackPanel HorizontalAlignment="Center">
		<TextBlock HorizontalAlignment="Center" Name="Text" Margin="4, 30, 4, 20" FontSize="22" FontWeight="700" />
		<StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Name="Buttons">
			<StackPanel.Styles>
				<Style Selector="Button">
					<Setter Property="Margin" Value="25"/>
				</Style>
			</StackPanel.Styles>

		</StackPanel>
	</StackPanel>
</Window>
